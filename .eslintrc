{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "browser": true,
      "jquery": true,
      "node": true,
      "jasmine": true
    },
      "rules": {
      "semi": 1,
      "indent": ["off", 2],
      "no-console": "warn",
      "no-debugger": "warn",
  }
}
